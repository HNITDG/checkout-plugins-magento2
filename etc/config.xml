<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <lmerchant>
                <active>1</active>
                <model>Lmerchant\Checkout\Model\Payment</model>
                <merchant_id />
                <merchant_secret backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <test_mode>1</test_mode>
                <order_status>pending_payment</order_status>
                <payment_action>authorize_capture</payment_action>
                <title>Lmerchant</title>
                <currency>AUD</currency>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_void>1</can_void>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>1</is_gateway>
                <url_sandbox>api.dev.latitudefinancial.com/v1/applybuy-checkout-service</url_sandbox>
                <url_prod>api.latitudefinancial.com/v1/applybuy-checkout-service</url_prod>
            </lmerchant>
        </payment>
    </default>
</config>
